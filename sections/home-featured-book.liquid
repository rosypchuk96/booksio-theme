              {% if section.settings.cover != blank %}
                <img class="pagebuilder-mobile-hidden box-model-64a73e700e545 box-model-64a73e701b094" src="{{ section.settings.cover | img_url: 'master' }}" alt="" title="" data-element="desktop_image" data-pb-style="GDT2O4J" data-loaded="true">
                <img class="pagebuilder-mobile-only box-model-64a73e700e54d box-model-64a73e701b09d" src="{{ section.settings.cover | img_url: 'master' }}" alt="" title="" data-element="mobile_image" data-pb-style="IW6BAWT">
              {% endif %}
            </figure>
          </div>
          <style type="text/css">.box-model-64a73e700e4d7 {margin-top: 10px;margin-right: 10px;margin-bottom: 10px;margin-left: 10px;padding-top: 20px;}@media only screen and (max-width: 768px) {.box-model-64a73e700e4d7 {margin-bottom: 45px;}}</style>
          <style type="text/css">.box-model-64a73e701b033 {margin-top: 10px;margin-right: 10px;margin-bottom: 10px;margin-left: 10px;padding-top: 20px;}@media only screen and (max-width: 768px) {.box-model-64a73e701b033 {margin-bottom: 45px;}}</style>
        </div>
      </div>
    </div>
    <style type="text/css">
      {% if section.settings.bg != blank %}
      .background-image-64a73e700e240 {
        background-image: url({{ section.settings.bg | img_url: 'master' }});
        background-position: center center;
      }
      {% endif %}
      {% if section.settings.bg_mobile != blank %}
      @media only screen and (max-width: 768px) and (min-width: 640px) {
        .background-image-64a73e700e240 {
          background-image: url({{ section.settings.bg_mobile | img_url: 'master' }});
        }
      }
      @media only screen and (max-width: 640px) {
        .background-image-64a73e700e240 {
          background-image: url({{ section.settings.bg_mobile | img_url: 'master' }});}}
      {% endif %}
    </style>
  </div>
</div>

