          <div class="pagebuilder-column gilded-cols box-model-64a73e700e4d7 box-model-64a73e701b033" data-content-type="column" data-appearance="align-center" data-background-images="{}" data-element="main" data-pb-style="G2PE9RD">
            <div data-content-type="text" data-appearance="default" data-element="main" data-pb-style="XQU4DU1" class="box-model-64a73e700e503 box-model-64a73e701b055">
              <p><span style="color: #ffffff; line-height: 14px;"><span style="font-size: 18px;"><strong>"{{ section.settings.quote }}"</strong></span></span></p>
              <p><span style="color: #ffffff; line-height: 14px;"><span style="font-size: 18px;"><strong>- {{ section.settings.quote_author }}</strong></span>&nbsp;</span></p>
              <p>&nbsp;</p>
              <div style="color: #fff;">{{ section.settings.text }}</div>
              <p>&nbsp;</p>
            </div>
            <div data-content-type="text" data-appearance="default" data-element="main" data-pb-style="KCNET0C" class="box-model-64a73e700e50d box-model-64a73e701b05e">
              <p><span style="font-size: 14px; color: #ffffff; line-height: 20px;">Featured Book</span></p>
            </div>
            <div data-content-type="text" data-appearance="default"  data-element="main" data-pb-style="TK6ET5M" class="box-model-64a73e700e517 box-model-64a73e701b067">
              <p><span style="color: #ffffff; font-size: 52px; line-height: 26px;"><strong>{{ section.settings.book_name }}</strong></span></p>
            </div>
            <div data-content-type="text" data-appearance="default" data-element="main" data-pb-style="OQSS9XO" class="box-model-64a73e700e520 box-model-64a73e701b070">
              <p><span style="font-size: 18px;"><strong><span style="color: #ffffff;">{{ section.settings.book_author }}</span></strong></span></p>
              <p>&nbsp;</p>
            </div>
            <div data-content-type="buttons" data-appearance="inline" data-same-width="false" data-element="main" data-pb-style="TRU7812" class="round-orange-btn box-model-64a73e700e528 box-model-64a73e701b079">
              <div class="round-orange-btn" data-content-type="button-item" data-appearance="default" data-element="main" data-pb-style="EBIF8FF">
                {% if section.settings.button_text != blank or section.settings.button_link != blank %}
                  <a class="pagebuilder-button-primary box-model-64a73e700e532 box-model-64a73e701b082" href="{{ section.settings.button_link }}" target="" data-link-type="default"  data-element="link" data-pb-style="EC8CIV5"><span data-element="link_text">{{ section.settings.button_text }}</span></a>
                {%endif %}
              </div>
            </div>
          </div>
          <div class="pagebuilder-column gilded-cols" data-content-type="column" data-appearance="align-center" data-background-images="{}" data-box-model="{}" data-element="main" data-pb-style="P73V7Y2">
