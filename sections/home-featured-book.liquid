      <style>
        @media (min-width: 769px) and (max-width: 1200px) {
        .gilded-block {
        min-height: 800px !important;
        }
        }
        @media (max-width: 767px) {
        .gilded-block .pagebuilder-column-group {
        flex-direction: column-reverse;
        }
        .gilded-block .gilded-cols {
        width: 100% !important;
        }
        .gilded-block [data-content-type=buttons] {
        text-align: center;
        }
        }
        .background-image-64a73e700e240 {
            background-image: url(https://www.booksio.com/media/wysiwyg/BKSIO_Homepage-BKGRD-July.png);
        }
      </style>
