<section
  id="MainProduct-{{ section.id }}"
  class="page-width section-{{ section.id }}-padding"
  data-section="{{ section.id }}"
>
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

 {{ 'product-template-style.css' | asset_url | stylesheet_tag }}
   {{ 'base.css' | asset_url | stylesheet_tag }}
    {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
    {{ 'section-related-products.css' | asset_url | stylesheet_tag }}
  
  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}

  <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
    <div class="grid__item product__media-wrapper{% if section.settings.media_position == 'right' %} medium-hide large-up-hide{% endif %}">
      {% render 'product-media-gallery', variant_images: variant_images %}
    </div>
    <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">

<div class="form">
        <form
          action="/cart/add"
          method="post"
          id="product-form-{{ product.id }}">

 <div class="product__title" {{ block.shopify_attributes }}>
                <h1>{{ product.title | escape }}
            
               {% if product.metafields.custom.author != blank %}
  <h2>by <span class="author">{{ product.metafields.custom.author }}</span></h2>
{% endif %}
            
            </h1>
                <a href="{{ product.url }}" class="product__title">
                  <h2 class="h1">
                    {{ product.title | escape }}
                  </h2>
<style>
            span.author{
            font-weight: bold;
            color: #298FC2;
            
            }
            </style>
                </a>
              </div>
<div style="margin-bottom: 2%;" class="category-label-wrapper-outter"><span class="category-label-wrapper">Creative Ability</span></div>
          <input
            type="hidden"
            name="id"
            value="{{ product.selected_or_first_available_variant.id }}">
          {%- unless product.has_only_default_variant -%}
            <div class="variant-selector">
  
          <!-- Product prices and compare prices -->
  <ul style="padding: 0px;">
    {% for variant in product.variants %}
      <li class="button">
        <input
          type="radio"
          id="variant-{{ variant.id }}"
          name="id"
          value="{{ variant.id }}"
          {% if product.selected_or_first_available_variant.id == variant.id %}checked{% endif %}>
        <label class="btn btn-default" for="variant-{{ variant.id }}">
          {% if variant.title == "Print" %}
            {{ variant.title }} Only
          {% else %}
           {{ variant.title }} <br/> 
              <span style="display: inline-block;width: 120px;font-weight: bold;">  
            {% if variant.compare_at_price > variant.price %}
         <del> {{ variant.compare_at_price | money }}</del>
            {% endif %}
            {{ variant.price | money }}</span>
          {% endif %}
        </label>
      </li>
    {% endfor %}
  </ul>

<!--               <div class="item" id="all-variants">
                <input
                  type="radio"
                  id="variant-all"
                  name="options"
                  value="{% for variant in product.variants %}{{ variant.id }},{% endfor %}">
                <label for="variant-all">
                    Print and Digital
                </label>
              </div> -->
            </div>
          {%- endunless -%}
          {% if product.selling_plan_groups %}
            <div class="subscription-widget" id="subscription-widget">
              {% assign selling_plans = product.selling_plan_groups[0].selling_plans %}
              <div class="block active" id="subscribe">
                <div class="name">Subscribe & Save</div>
                <div class="dropdown">
                  <div class="current dropdown-line">
                    {% assign years = selling_plans[0].name | replace: 'years', '' | replace: 'year', '' | replace: ' ', ''   %}
                    {% assign issues = product.metafields.accentuate.issues | times: years  %}
                    <div class="frequency">{{ selling_plans[0].options[0].value }}</div>
                    <div class="issues">{{ issues }} issues</div>
                    <div class="discount">{% if product.metafields.accentuate.first_order_discount_ %} Save {{ product.metafields.accentuate.first_order_discount_ }}%{% endif %}</div>
                    <div class="price">{{ selling_plans[0].price_adjustments[0].value | money }}</div>
                  </div>
                  <div class="dropdown-content">
                    {% for plan in selling_plans %}
                      {% unless plan.name contains 'Free' or plan.name contains 'free' %}
                        {% assign years = plan.name | replace: 'years', '' | replace: 'year', '' | replace: ' ', ''   %}
                        {% assign issues = product.metafields.accentuate.issues | times: years  %}
                        <div class="dropdown-line {% if forloop.index == 1 %}active{% endif %}" data-plan-id="{{ plan.id }}" data-plan-name="{{ plan.name }}">
                          <div class="line-wrap">
                            <div class="frequency">{{ plan.name }}</div>
                            <div class="issues">{{ issues }} issues</div>
                            <div class="discount">{% if product.metafields.accentuate.first_order_discount_ %}Save {{ product.metafields.accentuate.first_order_discount_ }}%{% endif %}</div>
                            <div class="price">{{ plan.price_adjustments[0].value | money }}</div>
                          </div>
                        </div>
                      {% endunless %}
                    {% endfor %}
                  </div>
                </div>
              </div>
<!--               <div class="block" id="single-issue">
                <div class="name">Single issue</div>
                <div class="single-issue">{{ product.selected_or_first_available_variant.price | money }}</div>
              </div> -->
            </div>
          {% endif %}
          <div class="row">
      
              <label class="label-quantity">Quantity</label>
              <input class="quantity-input"
                min="1"
                type="number"
                id="quantity"
                name="quantity"
                value="1" />


    
            
            <button
              type="submit"
              name="add"
              class="action primary tocart"
              id="product-addtocart-button"
              id="submit">ADD TO CART</button>
          </div>
        </form>
      </div>

<div data-content-type="html" data-appearance="default" data-box-model="{&quot;desktop&quot;:{&quot;margin&quot;:{&quot;top&quot;:&quot;0px&quot;,&quot;right&quot;:&quot;0px&quot;,&quot;bottom&quot;:&quot;0px&quot;,&quot;left&quot;:&quot;0px&quot;},&quot;padding&quot;:{&quot;top&quot;:&quot;0px&quot;,&quot;right&quot;:&quot;0px&quot;,&quot;bottom&quot;:&quot;0px&quot;,&quot;left&quot;:&quot;0px&quot;}}}" data-element="main" data-pb-style="KQEQY55" data-decoded="true" class="box-model-64c2dd8729630"><div class="secure-checkout-wrapper">
<p class="secure-title">Secure Checkout</p>
<div class="secure-thumbnail">
<img src="https://backend.booksio.com/media/wysiwyg/payment-method.png" alt="payment method" style="display: inline-block;">
</div>
</div>
<style>
.secure-checkout-wrapper {
    padding: 20px 30px;
    text-align: center;
    text-transform: capitalize;
    background: #eee;
    margin-top: 20px;
    margin-bottom: 40px;
    display: flex;
    flex-flow: row wrap;
    justify-content: center;
    color: #212322;
    font-family: 'Lato';
    max-width: 380px;
  }
.secure-checkout-wrapper .secure-title {
    width: 100%;
}
.charity-text p {
    color: #212322;
    font-size: 16px;
    line-height: 20px;
    font-weight: bold;
    margin-top: 12px;
    font-family: 'Lato';
}
